{
  "name": "jest-dynalite",
  "version": "1.1.1",
  "description": "Run your tests using Jest & Dynalite",
  "license": "MIT",
  "repository": "https://github.com/freshollie/jest-dynalite",
  "author": {
    "name": "Oliver Bell",
    "email": "freshollie@gmail.com",
    "url": "https://ollie.codes"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "coverage": "jest --coverage",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "test": "jest",
    "test:environment": "yarn pretest && jest -c jest-environment.config.js",
    "test:advanced": "yarn pretest && jest -c jest-advanced.config.js",
    "test:all": "yarn pretest && jest && jest -c jest-environment.config.js && jest -c jest-advanced.config.js",
    "build": "rm -rf dist && tsc",
    "pretest": "yarn build",
    "prepare": "yarn build"
  },
  "files": [
    "dist",
    "jest-preset.js"
  ],
  "keywords": [
    "jest",
    "dynamodb",
    "dynamodb local",
    "dynalite",
    "jest preset",
    "jest environment"
  ],
  "dependencies": {
    "@jest/types": "^24.9.0",
    "dynalite": "^3.0.0",
    "jest-environment-node": "^24.9.0"
  },
  "peerDependencies": {
    "aws-sdk": "2.x.x"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/cwd": "^0.10.0",
    "@types/jest": "^24.0.18",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.3.3",
    "aws-sdk": "2.548.0",
    "commitlint": "^8.2.0",
    "eslint": "6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "3.0.8",
    "jest": "24.9.0",
    "lint-staged": "9.4.2",
    "prettier": "^1.19.1",
    "typescript": "^3.6.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ],
    "*.{html,json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
